- site_title = "InxyADS";
- active__menu = 'Dashboard';
- page_title = 'Dashboard';
doctype html
html(lang="en")
  include includes/head
  body.loading
    .inxy-preloader
      .inxy-preloader__circle
    include includes/navbar
    .inxy-page.layout-dashboard
      section.content-header
        h1=page_title
        ul.breadcrumb
          li: a(href="")
            i.fa.fa-home 
            | Dashboard 
          
      .container
        h2.filters-header Статистика 
          small за всё время
          .dropdown
            svg.dropdown-mobile(type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"): use(xlink:href="#menu_icon")
            ul.dropdown-menu(aria-labelledby='dLabel')
              li.active: a(href="#", title="Всё время") Всё время
              li: a(href="#", title="Сегодня") Сегодня
              li: a(href="#", title="Вчера") Вчера
              li: a(href="#", title="Сегодня") Сегодня
              li: a(href="#", title="Неделя") Неделя
              li: a(href="#", title="Месяц") Месяц
              li: a(href="#", title="Квартал") Квартал
              li: a(href="#", title="Год") Год
        .filters
          ul.filters-nav
            li.active: a(href="#", title="Всё время") Всё время
            li: a(href="#", title="Сегодня") Сегодня
            li: a(href="#", title="Вчера") Вчера
            li: a(href="#", title="Сегодня") Сегодня
            li: a(href="#", title="Неделя") Неделя
            li: a(href="#", title="Месяц") Месяц
            li: a(href="#", title="Квартал") Квартал
            li: a(href="#", title="Год") Год
            li
              form.filters-custom(action="")
                input(type="text" id="daterange")
                i.fa.fa-calendar
          
          .row.stretch
            .col.md-5
              .panel
                .panel-heading
                  h3 показов: 
                    span 345 332 
                    small.success +12 032
                  .dropdown
                    svg.dropdown-toggle(type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"): use(xlink:href="#edit_icon")
                    ul.dropdown-menu(aria-labelledby='dLabel')
                      li.active: a(href="#", title="Всё время") Всё время
                      li: a(href="#", title="Сегодня") Сегодня
                      li: a(href="#", title="Вчера") Вчера
                      li: a(href="#", title="Сегодня") Сегодня
                      li: a(href="#", title="Неделя") Неделя
                .panel-body
                  #chartdiv1(style="width: 100%; height: 200px; background-color: #FFFFFF;")
            .col.md-4.lg-5
              .panel
                .panel-heading
                  h3 переходов: 
                    span 345 332 
                    small.danger -12 032
                  .dropdown
                    svg.dropdown-toggle(type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"): use(xlink:href="#edit_icon")
                    ul.dropdown-menu(aria-labelledby='dLabel')
                      li.active: a(href="#", title="Всё время") Всё время
                      li: a(href="#", title="Сегодня") Сегодня
                      li: a(href="#", title="Вчера") Вчера
                      li: a(href="#", title="Сегодня") Сегодня
                      li: a(href="#", title="Неделя") Неделя
                .panel-body
                  #chartdiv2(style="width: 100%; height: 200px; background-color: #FFFFFF;")
            .col.md-3.lg-2
              .panel.stats-panel
                .panel-heading
                  h3 статистика
                  .dropdown
                    svg.dropdown-toggle(type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"): use(xlink:href="#edit_icon")
                    ul.dropdown-menu(aria-labelledby='dLabel')
                      li.active: a(href="#", title="Всё время") Всё время
                      li: a(href="#", title="Сегодня") Сегодня
                      li: a(href="#", title="Вчера") Вчера
                      li: a(href="#", title="Сегодня") Сегодня
                      li: a(href="#", title="Неделя") Неделя
                .stats
                  .stats-body
                    .stats-title CTR
                    .stats-count  0,012%
                    .stats-perc.success +2,93 %
                  .stats-graph <span class="bar success">4,3,6,4,5,4,7,3,5,7,3,5,2</span>
                .stats
                  .stats-body
                    .stats-title Потрачено
                    .stats-count 122,5 $
                    .stats-perc.danger -8,67 %
                  .stats-graph <span class="bar danger">-4,-3,-6,-4,-5,-4,-7,-3,-5,-7,-3,-5,-2</span>

        .responsive-vertical: table.material-table.hover.bordered.striped.companies-table
          thead
            tr
              th Статус
              th Кампания  
                span.tooltip-bottom(data-tooltip="Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quos perspiciatis, quae unde porro illo!"): i.fa.fa-info-circle
              th Формат  
                span.tooltip-bottom(data-tooltip="Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quos perspiciatis, quae unde porro illo!"): i.fa.fa-info-circle
              th Ставка  
                span.tooltip-bottom(data-tooltip="Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quos perspiciatis, quae unde porro illo!"): i.fa.fa-info-circle
              th Потрачено  
                span.tooltip-bottom(data-tooltip="Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quos perspiciatis, quae unde porro illo!"): i.fa.fa-info-circle
              th CTR  
                span.tooltip-bottom(data-tooltip="Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quos perspiciatis, quae unde porro illo!"): i.fa.fa-info-circle
              th Показы  
                span.tooltip-bottom(data-tooltip="Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quos perspiciatis, quae unde porro illo!"): i.fa.fa-info-circle
              th Клики  
                span.tooltip-bottom(data-tooltip="Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quos perspiciatis, quae unde porro illo!"): i.fa.fa-info-circle
              th Редактировать 
          tbody
            - for (var x = 1; x < 5; x++)
              tr
                td(data-title='Статус')
                  .switch
                    input.switch__ctrl(checked type='checkbox' id='company__' + x)
                    label.switch__lbl(for='company__' + x)
                td(data-title='Кампания')
                  span.company__title CandyCrash на флеше
                    small http://inxyads.com/advertiser/statistics
                td(data-title='Формат')
                  span.company__format Branding
                    small мобильные
                td(data-title='Ставка')
                  span.company__rate 1,5 $ 
                    small CPA
                td(data-title='Потрачено')
                  span.company__spent 5,25 $
                td(data-title='CTR')
                  span.company__ctr= (12 * x / 1000) + '%'
                td(data-title='Показы')
                  span.company__impressions= 34555 * x
                td(data-title='Клики')
                  span.company__clicks= 417 + x
                td(data-title=' ')
                  a(href="compain-edit.html", title="edit").company__edit: svg: use(xlink:href="#edit_icon")
            - for (var x = 6; x < 10; x++)
              tr
                td(data-title='Статус')
                  .switch
                    input.switch__ctrl(type='checkbox' id='company__' + x)
                    label.switch__lbl(for='company__' + x)
                td(data-title='Кампания')
                  span.company__title CandyCrash на флеше
                    small http://inxyads.com/advertiser/statistics
                td(data-title='Формат')
                  span.company__format PopUnder
                    small компьютеры
                td(data-title='Ставка')
                  span.company__rate 1,5 $ 
                    small CPA
                td(data-title='Потрачено')
                  span.company__spent 5,25 $
                td(data-title='CTR')
                  span.company__ctr —
                td(data-title='Показы')
                  span.company__impressions= 34555 * x
                td(data-title='Клики')
                  span.company__clicks —
                td(data-title=' ')
                  a(href="compain-edit.html", title="edit").company__edit: svg: use(xlink:href="#edit_icon")
            - for (var x = 11; x < 15; x++)
              tr
                td(data-title='Статус')
                  .switch
                    input.switch__ctrl( disabled type='checkbox' id='company__' + x)
                    label.switch__lbl(for='company__' + x)
                td(data-title='Кампания')
                  span.company__title CandyCrash на флеше
                    small http://inxyads.com/advertiser/statistics
                td(data-title='Формат')
                  span.company__format Branding
                    small мобильные
                td(data-title='Ставка')
                  span.company__rate 1,5 $ 
                    small CPA
                td(data-title='Потрачено')
                  span.company__spent 5,25 $
                td(data-title='CTR')
                  span.company__ctr= (12 * x / 1000) + '%'
                td(data-title='Показы')
                  span.company__impressions= 34555 * x
                td(data-title='Клики')
                  span.company__clicks= 417 + x
                td(data-title=' ')
                  a(href="compain-edit.html", title="edit").company__edit: svg: use(xlink:href="#edit_icon")
    include includes/footer
    include includes/scripts
